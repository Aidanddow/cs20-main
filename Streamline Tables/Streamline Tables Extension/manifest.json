{
  "manifest_version": 2,

  "name": "Streamline Tables",
  "description": "Download Tables",
  "version": "1.0",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },

  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },

  "content_scripts": [ {
    "all_frames": true,
    "js": [ "content.js", "jquery-3.1.0.min.js" ],
    "matches": [ "\u003Call_urls>" ],
    "run_at": "document_end"
 } ],

  "background": {
    "scripts": ["background.js", "jquery-3.1.0.min.js"],
    "persistent": false
  },

  "content_security_policy": "script-src 'self' https://*.google.com 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "contextMenus",
		"tabs",
		"http://127.0.0.1:8000/",
		"background",
        "identity",
        "notifications"
	]
}